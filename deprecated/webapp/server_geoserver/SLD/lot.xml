<?xml version="1.0" encoding="ISO-8859-1"?>
<StyledLayerDescriptor version="1.0.0" 
		xsi:schemaLocation="http://www.opengis.net/sld StyledLayerDescriptor.xsd" 
		xmlns="http://www.opengis.net/sld" 
		xmlns:ogc="http://www.opengis.net/ogc" 
		xmlns:xlink="http://www.w3.org/1999/xlink" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<NamedLayer>	
		<Name>Lot</Name>
		<UserStyle>
			<Name>propertyIsEqualTo</Name>
			<Title>propertyIsEqualTo</Title>
			<FeatureTypeStyle>
			
                <Rule>	
					<Title>En partie</Title>	
					<ogc:Filter>
							<ogc:PropertyIsEqualTo>
								<ogc:PropertyName>category</ogc:PropertyName>
								<ogc:Literal>O</ogc:Literal>
							</ogc:PropertyIsEqualTo>
					</ogc:Filter>							
					<LineSymbolizer>
						<Stroke>
							<CssParameter name="stroke">#F0F0F0F</CssParameter>
							<CssParameter name="stroke-opacity">0.8</CssParameter>
							<CssParameter name="stroke-width">2</CssParameter>
						</Stroke>
					</LineSymbolizer>					
				</Rule>
				<Rule>
                    <Title>Complet</Title>	
					<ogc:Filter>
							<ogc:PropertyIsEqualTo>
								<ogc:PropertyName>category</ogc:PropertyName>
								<ogc:Literal>N</ogc:Literal>
							</ogc:PropertyIsEqualTo>
					</ogc:Filter>	
                    <LineSymbolizer>
						<Stroke>
							<CssParameter name="stroke">#0F0F0F</CssParameter>
							<CssParameter name="stroke-opacity">0.8</CssParameter>
							<CssParameter name="stroke-width">1</CssParameter>
						</Stroke>
					</LineSymbolizer>					
				</Rule>
			</FeatureTypeStyle>
		</UserStyle>
	</NamedLayer>
</StyledLayerDescriptor>

